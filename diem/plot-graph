	0 [label=<0> fontsize=10 shape=rectangle]
	111 [label=<111> fontsize=10 shape=rectangle]
	11 [label=<11> fontsize=10 shape=rectangle]
	"[01]↔[0]+[1]" [label=<<b>[01]↔[0]+[1]</b><br/>> fontsize=10 shape=plaintext]
	"[01]↔[0]+[1]" -> 01
	"[01]↔[0]+[1]" -> 0
	"[01]↔[0]+[1]" -> 1
	"[0001]↔[0]+[001]" [label=<<b>[0001]↔[0]+[001]</b><br/>> fontsize=10 shape=plaintext]
