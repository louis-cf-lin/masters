	"[111]↔[1]+[11]" [label=<<b>[111]↔[1]+[11]</b><br/>> fontsize=10 shape=plaintext]
	"[111]↔[1]+[11]" -> 111
	"[111]↔[1]+[11]" -> 1
	"[111]↔[1]+[11]" -> 11
	"[011]+[111]↔[11]+[0111]" [label=<<b>[011]+[111]↔[11]+[0111]</b><br/>> fontsize=10 shape=plaintext]
	"[011]+[111]↔[11]+[0111]" -> 011
	"[011]+[111]↔[11]+[0111]" -> 111
	"[011]+[111]↔[11]+[0111]" -> 11
	"[011]+[111]↔[11]+[0111]" -> 0111
